C-------------------------------------------------------------
C***  SET PRIMARY GRID DIMENSIONS AND PRESSURE OUTPUT LEVELS
      PARAMETER (IM=imin,JM=jmin,LM=lmin,LSM=lsmin)
C-------------------------------------------------------------
C***  SET THE NUMBER OF PEs IN THE I-DIRECTION AND J-DIRECTION
      PARAMETER(INPES=ipcin,JNPES=jpcin)
C-------------------------------------------------------------
C
      PARAMETER(IGSTL=-5,IGSTR=5)
      PARAMETER(JGSTL=-5,JGSTR=5)
      PARAMETER(ITAIL=IM-(INPES*(IM/INPES)))
      PARAMETER(JTAIL=JM-(JNPES*(JM/JNPES)))
C
      PARAMETER(IDIM1=IGSTL,IDIM2=IM/INPES+IGSTR+1)
      PARAMETER(JDIM1=JGSTL,JDIM2=JM/JNPES+JGSTR+1)
C
C-------------------------------------------------------------
